<div class="clearfix">
  <article id="post">
    <h1><%=page.title%></h1>
    <%-page.content%>
    <div class="bottom-line"></div>
    <%- partial('partials/post/nav', {post: page}) %>
  </article>
  <div id="toc">
    <% if(toc(page.content)) { %>
      <h2>文章目录</h2>
      <%- toc(page.content) %>
    <%} else {%>
      <h2>没有目录...</h2>
    <% } %>
  </div>
</div>
<%-paginator()%>
